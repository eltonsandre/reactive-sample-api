<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Or>
            <Class name="~.*Test$"/>
        </Or>
        <Or>
            <Class name="~.*Test\$.*$"/>
        </Or>
        <Or>
            <Class name="~.*Stub$"/>
        </Or>
        <Or>
            <Class name="~.*Stub\$.*$"/>
        </Or>
    </Match>
</FindBugsFilter>
